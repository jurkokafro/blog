[32m+[m[32muse Illuminate\Validation\ValidationException;[m
[32m+[m
[32m+[m[32mclass SessionController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    public function create() {[m
[32m+[m[32m        return view('sessions.create');[m
[32m+[m[32m    }[m
[32m+[m[32m    public function destroy() {[m
[32m+[m[32m        auth()->logout();[m
[32m+[m
[32m+[m[32m        return redirect('/')->with("success","Goodbye");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function store() {[m
[32m+[m
[32m+[m[32m        $attributes = request()->validate([[m
[32m+[m[32m            'email' => ['required','email'],[m
[32m+[m[32m            'password' => ['required'],[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        if(! auth()->attempt($attributes)) {[m
[32m+[m[32m            throw ValidationException::withMessages([[m
[32m+[m[32m                'email' => 'We are really sorry, but your provided credentials could not be verified!'[m
[32m+[m[32m            ]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        session()->regenerate(); //zaÅ¡Äita pred session fixation[m
[32m+[m
[32m+[m[32m        return redirect('/')->with('success', 'Welcome back');[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
