[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse App\Models\Category;[m
[32m+[m[32muse App\Models\Post;[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m
[32m+[m[32mclass PostController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    public function index() {[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        return view('posts.index', [[m
[32m+[m
[32m+[m[32m            'posts' => Post::latest()->filter([m
[32m+[m[32m                request(['search','category','author']))[m
[32m+[m[32m            ->paginate()->withQueryString(),[m
[32m+[m
[32m+[m
[32m+[m[32m        ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function show(Post $post) {[m
[32m+[m[32m         //Find a post by its slug and pass it to a view called post[m
[32m+[m[32m        return view('posts.show', [[m
[32m+[m[32m            //'post' => Post::findOrFail($id)[m
[32m+[m[32m            'post' => $post,[m
[32m+[m[32m        ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
[1mdiff --git a/app/Http/Controllers/RegisterController.php b/app/Http/Controllers/RegisterController.php[m
[1mnew file mode 100644[m
[1mindex 0000000..83f2cb3[m
[1m--- /dev/null[m
[1m+++ b/app/Http/Controllers/RegisterController.php[m
[36m@@ -0,0 +1,42 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse App\Models\User;[m
[32m+[m[32muse Illuminate\Auth\Middleware\Authenticate;[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\Auth;[m
[32m+[m[32muse Illuminate\Validation\Rule;[m
[32m+[m
[32m+[m[32muse function PHPUnit\Framework\returnSelf;[m
[32m+[m
[32m+[m[32mclass RegisterController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    //[m
[32m+[m[32m    public function create() {[m
[32m+[m[32m        return view('register.create');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function store() {[m
[32m+[m
[32m+[m
[32m+[m[32m        $attributes = request()->validate([[m
[32m+[m[32m            'name' => ['required','max:255'],[m
[32m+[m[32m            'username' => ['required','max:255','min:3', Rule::unique('users', 'username')],[m
[32m+[m[32m            'email' => ['required','email','max:255', Rule::unique('users', 'email')],[m
[32m+[m[32m            'password' => ['required','min:7'],[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        //$attributes['password'] = bcrypt($attributes['password']); --> zdaj za to poskrbi User.php funcija set[m
[32m+[m[32m        //setPasswordAttribute ... - avtomatsko poskrbi laravel za to[m
[32m+[m
[32m+[m[32m        $user = User::create($attributes);[m
[32m+[m
[32m+[m[32m        //Login user[m
[32m+[m[32m        auth()->login($user);[m
[32m+[m
[32m+[m[32m        //session()->flash("success", "Your account has been created!");[m
[32m+[m
[32m+[m[32m        return redirect('/')->with("success", "Your account has been created!");[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
