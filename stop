[32m+[m[32m                    <li class="disabled" aria-disabled="true"><span>{{ $element }}</span></li>[m
[32m+[m[32m                @endif[m
[32m+[m
[32m+[m[32m                {{-- Array Of Links --}}[m
[32m+[m[32m                @if (is_array($element))[m
[32m+[m[32m                    @foreach ($element as $page => $url)[m
[32m+[m[32m                        @if ($page == $paginator->currentPage())[m
[32m+[m[32m                            <li class="active" aria-current="page"><span>{{ $page }}</span></li>[m
[32m+[m[32m                        @else[m
[32m+[m[32m                            <li><a href="{{ $url }}">{{ $page }}</a></li>[m
[32m+[m[32m                        @endif[m
[32m+[m[32m                    @endforeach[m
[32m+[m[32m                @endif[m
[32m+[m[32m            @endforeach[m
[32m+[m
