[32m+[m[32muse App\Models\Post;[m
[32m+[m
[32m+[m[32mclass DatabaseSeeder extends Seeder[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Seed the application's database.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return void[m
[32m+[m[32m     */[m
[32m+[m[32m    public function run()[m
[32m+[m[32m    {[m
[32m+[m[32m        User::truncate();[m
[32m+[m[32m        Category::truncate();[m
[32m+[m[32m        Post::truncate();[m
[32m+[m
[32m+[m[32m        Post::factory(125)->create();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        /*[m
[32m+[m[32m        $user = User::factory()->create();[m
[32m+[m
[32m+[m[32m        // \App\Models\User::factory()->create([[m
[32m+[m[32m        //     'name' => 'Test User',[m
[32m+[m[32m        //     'email' => 'test@example.com',[m
[32m+[m[32m        // ]);[m
[32m+[m
[32m+[m[32m       $personal = Category::create([[m
[32m+[m[32m            'name' => 'Personal',[m
[32m+[m[32m            'slug' => 'personal'[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        $work = Category::create([[m
[32m+[m[32m            'name' => 'Work',[m
[32m+[m[32m            'slug' => 'work'[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        $hobbies = Category::create([[m
[32m+[m[32m            'name' => 'Hobbies',[m
[32m+[m[32m            'slug' => 'hobbies'[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        Post::create([[m
[32m+[m[32m            'user_id' => $user->id,[m
[32m+[m[32m            'category_id' => $personal->id,[m
[32m+[m[32m            'title' => 'My Family Post',[m
[32m+[m[32m            'slug' => 'my-family-post',[m
[32m+[m[32m            'excerpt' => '<p>Lorem Ipsum doler sit amet </p>',[m
[32m+[m[32m            'body' => '<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry.[m
[32m+[m[32m            Lorem Ipsum has been the industry\'s standard dummy text ever since the 1500s, when an unknown[m
[32m+[m[32m            printer took a galley of type and scrambled it to make a type specimen book.[m
[32m+[m[32m            It has survived not only five centuries, but also the leap into electronic typesetting,[m
[32m+[m[32m            remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset[m
[32m+[m[32m            sheets containing Lorem Ipsum passages, and more recently with desktop publishing software like[m
[32m+[m[32m            Aldus PageMaker including versions of Lorem Ipsum.</p>'[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        Post::create([[m
[32m+[m[32m            'user_id' => $user->id,[m
[32m+[m[32m            'category_id' => $work->id,[m
[32m+[m[32m            'title' => 'My Work Post',[m
[32m+[m[32m            'slug' => 'my-work-post',[m
[32m+[m[32m            'excerpt' => '<p>Lorem Ipsum doler sit amet</p>',[m
[32m+[m[32m            'body' => '<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry.[m
[32m+[m[32m            Lorem Ipsum has been the industry\'s standard dummy text ever since the 1500s, when an unknown[m
