[32m+[m[32m            'username' => env('DB_USERNAME', 'forge'),[m
[32m+[m[32m            'password' => env('DB_PASSWORD', ''),[m
[32m+[m[32m            'unix_socket' => env('DB_SOCKET', ''),[m
