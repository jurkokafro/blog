[32m+[m[32m            'table' => 'jobs',[m
[32m+[m[32m            'queue' => 'default',[m
[32m+[m[32m            'retry_after' => 90,[m
[32m+[m[32m            'after_commit' => false,[m
[32m+[m[32m        ],[m
