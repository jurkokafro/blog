[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Models;[m
[32m+[m
[32m+[m[32muse Illuminate\Database\Eloquent\Factories\HasFactory;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Model;[m
[32m+[m
[32m+[m[32mclass Post extends Model[m
[32m+[m[32m{[m
[32m+[m[32m    use HasFactory;[m
[32m+[m
[32m+[m[32m    //protected $fillable = ['title', 'excerpt', 'body']; alternativa je quarder, kjer je obratno[m
[32m+[m[32m    //zaÅ¡ÄitiÅ¡ Äesa noÄeÅ¡ not[m
[32m+[m[32m    protected $guarded = ['id']; //ne Å¾elimo, da se id dodaja s strani uporabnika[m
[32m+[m
[32m+[m
[32m+[m[32m    protected $with = ['category', 'author'];[m
[32m+[m
[32m+[m
[32m+[m[32m    /* Alternativni naÄin temu, da namesto idja[m
[32m+[m[32m    uporabimo slug pri izbiri bloga iz baze[m
[32m+[m
[32m+[m[32m    Nedolgo nazaj je bil to edini naÄin[m
[32m+[m[32m    */[m
[32m+[m[32m    /*public function getRouteKeyName()[m
[32m+[m[32m    {[m
[32m+[m[32m        return 'slug';[m
[32m+[m[32m    }*/[m
[32m+[m
[32m+[m[32m    public function scopeFilter($query, array $filters) {[m
[32m+[m[32m        $query->when($filters['search'] ?? false, fn ($query, $search) =>[m
[32m+[m[32m        $query->where(fn($query) =>[m
[32m+[m[32m                $query->where('title', 'like', '%' . $search . '%' )[m
[32m+[m[32m                ->orWhere('body', 'like', '%' . $search . '%' )));[m
[32m+[m
[32m+[m[32m        $query->when($filters['category'] ?? false, fn ($query, $category) =>[m
[32m+[m[32m            $query->whereHas('category', fn($query) =>[m
[32m+[m[32m                $query->where('slug', $category)));[m
[32m+[m
[32m+[m[32m        $query->when($filters['author'] ?? false, fn ($query, $author) =>[m
[32m+[m[32m            $query->whereHas('author', fn($query) =>[m
[32m+[m[32m                $query->where('username', $author)));[m
[32m+[m[32m       /* teÅ¾ja metoda -  $query[m
[32m+[m[32m                ->whereExists(fn($query)=>[m
[32m+[m[32m                    $query->from('categories')[m
[32m+[m[32m                            ->whereColumn('categories.id', 'posts.category_id')[m
[32m+[m[32m                            ->where('categories.slug', $category)[m
[32m+[m[32m        ));*/[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function category() {[m
[32m+[m[32m        //hasOne, hasMany, belongsTo, belongsToMany[m
[32m+[m[32m        return $this->belongsTo(Category::class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function author() {[m
[32m+[m[32m        return $this->belongsTo(User::class, 'user_id');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
